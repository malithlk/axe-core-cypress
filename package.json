{
  "name": "axe-cypress-a11y",
  "version": "1.0.0",
  "description": "axe-core plugin for cypress",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/malithlk/axe-core-cypress#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malithlk/axe-core-cypress.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint . --cache --fix",
    "pretest": "npm run lint",
    "test:e2e:ci": "npx cypress run --spec 'cypress/e2e/axe-cypress.cy.ts' --headless",
    "test": "npm run test:e2e:ci"
  },
  "dependencies": {
    "axe-core": "~4.11.1"
  },
  "peerDependencies": {
    "cypress": ">=12.0.0",
    "axe-core": ">=4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "cypress": "^15.8.0",
    "eslint": "^7.12.0",
    "eslint-config-tamia": "^7.2.6",
    "http-server": "^0.12.3",
    "globals": "^16.3.0",
    "husky": "^9.0.10",
    "lint-staged": "^16.1.2",
    "prettier": "^3.0.3",
    "start-server-and-test": "^1.11.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "@types/mocha": "^10.0.0"
  },
  "keywords": [
    "a11y",
    "cypress",
    "testing",
    "plugin",
    "axe-core"
  ],
  "authors": [
    {
      "name": "Malith Karunaratne",
      "url": "https://github.com/malithlk"
    }
  ],
  "license": "MIT"
}
